# OpenAI API Key (if using OpenAI models)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (if using Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Tavily API Key (for web search with search_provider="tavily" or "hybrid")
# Get your key at: https://tavily.com/
TAVILY_API_KEY=your_tavily_api_key_here

# Google API Key (optional, for search_provider="google_adk" or Gemini models)
# Google ADK google_search is FREE - no API key required for basic search
# Only needed if using Gemini models (gemini-2.0-flash, etc.)
# Get your key at: https://makersuite.google.com/app/apikey
# GOOGLE_API_KEY=your_google_api_key_here

# Optional: LangSmith for tracing (recommended for debugging)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=company-search-agent
